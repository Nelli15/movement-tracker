{
  "name": "app",
  "version": "4.0.4",
  "private": true,
  "description": "An app for tracking Christian discipleship trees",
  "author": "Nick Ellis <ellis.nick96@gmail.com>",
  "productName": "Movement Tracker",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "build": "quasar build -m pwa",
    "dev:emulated": "firebase emulators:exec --only auth,functions,firestore,database --ui \"quasar dev\"",
    "dev:pwa:emulated": "firebase emulators:exec --only auth,functions,firestore,database --ui \"quasar dev -m pwa\"",
    "start:emulators": "firebase emulators:start --only auth,functions,firestore,database",
    "test:e2e": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" FIREBASE_DATABASE_EMULATOR_HOST=\"localhost:9000\" E2E_TEST=true start-test \"quasar dev\" http-get://localhost:5000 \"cypress open\"",
    "test:e2e:ci": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:5000 \"cypress run\"",
    "test:e2e:emulated": "firebase emulators:exec --only auth,functions,firestore,database --ui \"npm run test:e2e\"",
    "test:e2e:ci:emulated": "firebase emulators:exec --only auth,functions,firestore,database --ui \"npm run test:e2e:ci\"",
    "test:jest": "jest --watchAll --verbose=false",
    "test:jest:emulated": "firebase emulators:exec --only auth,functions,firestore,database \"jest\"",
    "test:jest:emulated:dev": "firebase emulators:exec --only auth,functions,firestore,database \"jest --watchAll\""
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.18.9",
    "@firebase/rules-unit-testing": "^2.0.3",
    "@quasar/app-webpack": "^3.5.7",
    "@quasar/extras": "^1.15.0",
    "babel-jest": "^28.1.3",
    "chart.js": "^3.8.0",
    "core-js": "^3.23.5",
    "esm": "^3.2.25",
    "file-saver": "^2.0.5",
    "firebase": "^9.9.0",
    "quasar": "^2.7.5",
    "ts-jest": "^28.0.7",
    "vue": "^3.2.37",
    "vue-chart-3": "^3.0.9",
    "vue-router": "^4.1.2",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "@quasar/quasar-app-extension-qcalendar": "^4.0.0-beta.3",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^4.2.1",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "cross-env": "^7.0.3",
    "cypress": "^9.7.0",
    "cypress-firebase": "^2.2.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.4.1",
    "firebase-admin": "^10.3.0",
    "js-base64": "^3.7.2",
    "stylus": "^0.57.0",
    "stylus-loader": "^6.1.0",
    "workbox-webpack-plugin": "^6.2.2"
  },
  "overrides": {
    "async@<3.2.2": "3.2.2",
    "minimist@1.2.5": "1.2.6",
    "node-ipc@>9.2.1 <10": "9.2.1",
    "node-ipc@>10.1.0": "10.1.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">=16.14.2",
    "npm": ">=8.7.0",
    "yarn": ">= 1.21.1"
  }
}
