{
  "name": "movement-tracker",
  "version": "4.0.0",
  "private": true,
  "description": "An app for tracking Christian discipleship trees",
  "author": "Nick Ellis <ellis.nick96@gmail.com>",
  "productName": "Movement Tracker",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "build": "quasar build -m pwa",
    "dev:emulated": "firebase emulators:exec --only auth,functions,firestore,database --ui \"quasar dev\"",
    "dev:pwa:emulated": "firebase emulators:exec --only auth,functions,firestore,database --ui \"quasar dev -m pwa\"",
    "start:emulators": "firebase emulators:start --only auth,functions,firestore,database",
    "start:emulators-import": "firebase emulators:start --only auth,functions,firestore,pubsub --import ./firestoreData",
    "test:e2e": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" FIREBASE_DATABASE_EMULATOR_HOST=\"localhost:9000\" E2E_TEST=true start-test \"quasar dev\" http-get://localhost:5000 \"cypress open\"",
    "test:e2e:ci": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:5000 \"cypress run\"",
    "test:e2e:emulated": "firebase emulators:exec --only auth,functions,firestore,database \"npm run test:e2e\"",
    "test:rules:fire": "firebase emulators:exec --only firestore \"jest firestore.rules._jest.spec.js --detectOpenHandles\"",
    "test:rules:rtdb": "firebase emulators:exec --only database \"jest database.rules._jest.spec.js --detectOpenHandles\"",
    "test:rules:fire:watch": "firebase emulators:exec --only firestore \"jest firestore.rules._jest.spec.js --detectOpenHandles --watchAll\"",
    "test:rules:rtdb:watch": "firebase emulators:exec --only database \"jest database.rules._jest.spec.js --detectOpenHandles --watchAll\"",
    "test:jest": "jest --watchAll --verbose=false",
    "test:functions:emulated": "firebase emulators:exec --only auth,functions,firestore,database \"jest --watchAll\""
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@firebase/rules-unit-testing": "^2.0.1",
    "@quasar/extras": "^1.12.3",
    "babel-jest": "^27.2.4",
    "chart.js": "^3.6.0",
    "eslint-plugin-vue": "^8.3.0",
    "esm": "^3.2.25",
    "file-saver": "^2.0.5",
    "firebase": "^9.6.3",
    "quasar": "^2.4.10",
    "ts-jest": "^27.1.3",
    "vuedraggable": "^4.1.0",
    "core-js": "^3.6.5",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@quasar/app": "^3.1.8",
    "@quasar/quasar-app-extension-qcalendar": "^4.0.0-beta.3",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^3.1.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.2.1",
    "cypress-firebase": "^2.2.1",
    "eslint-plugin-cypress": "^2.12.1",
    "firebase-admin": "^10.0.0",
    "stylus": "^0.56.0",
    "stylus-loader": "^6.1.0",
    "workbox-webpack-plugin": "^6.2.2",
    "@babel/eslint-parser": "^7.13.14",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^7.0.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
